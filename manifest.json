{
    "manifest_version": 3,
    "version": "1.0",
    "name": "VKCart",
    "description": "Save items from any webpage to personalized lists and edit them outside the website.",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon128.png"
    },
    "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png" 
    },
    "permissions": [
        "tabs"
    ]
}
